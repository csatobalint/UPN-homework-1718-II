%% UNP-homework-I

%%
clear all;
close all;
clc;
format long

%time range
t0=0;
T=400;

%parameters of the mechanical system
m=2;
k=0.2;
s=100;
f0=60;
w=5;
warr=linspace(0,w,1000);
lambda=warr/sqrt(s/m);
D=k/(2*sqrt(s/m)*m);
N=1/sqrt((1-lambda.^2).^2+4*D^2*lambda.^2);


%%
%initial conditions
c1=2;
c2=2;

%timestep
%h=0.005;
%N=(T-t0)/h;
N=5000;

%forced vibration
fvbrRK4(T,N,m,k,s,f0,w,c1,c2)
